<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yonyou.ssc.mapper.CityMapper">
    <select id="selectCityById" resultType="City">
        select * from city where id = #{id}
    </select>
    <insert id="saveCity" useGeneratedKeys="true" keyProperty="id" parameterType="com.yonyou.ssc.entry.City" >
        insert into city(name,state,country,address) values(#{name,jdbcType=VARCHAR},#{state,jdbcType=VARCHAR},#{country,jdbcType=VARCHAR},#{address,jdbcType=VARCHAR})
    </insert>
</mapper>
